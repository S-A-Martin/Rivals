[
    {
        "key": "ctrl+l",
        "command": "workbench.action.terminal.clear",
        "when": "terminalFocus && terminalHasBeenCreated && terminalTextSelected || terminalFocus && terminalProcessSupported && terminalTextSelected"
    },
    {
        "key": "ctrl+alt+s",
        "command": "multiCommand.callTestBat",
        "when": "config.workspaceKeybindings.buildRunTestsMultiCommand.enabled"
    },
    {
        "key": "ctrl+alt+shift+s",
        "command": "multiCommand.callTestCleanBat",
        "when": "config.workspaceKeybindings.buildRunTestsMultiCommand.enabled"
    },
    {
        "key": "ctrl+alt+r",
        "command": "multiCommand.callRunBat",
        "when": "config.workspaceKeybindings.buildRunTestsMultiCommand.enabled"
    },
    {
        "key": "ctrl+alt+shift+r",
        "command": "multiCommand.callRunCleanBat",
        "when": "config.workspaceKeybindings.buildRunTestsMultiCommand.enabled"
    }
]